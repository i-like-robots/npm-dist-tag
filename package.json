{
  "name": "npm-dist-tag",
  "version": "1.1.1",
  "description": "A tool to find the appropriate npm distribution tag when publishing a new version of your package.",
  "bin": {
    "npm-dist-tag": "bin/npm-dist-tag.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "prettier --config .prettierrc.json --list-different lib/*.js",
    "spec": "jasmine 'tests/*.spec.js'",
    "test": "npm run lint && npm run spec"
  },
  "keywords": [
    "npm",
    "dist-tag",
    "publish"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/npm-dist-tag.git"
  },
  "homepage": "https://github.com/i-like-robots/npm-dist-tag",
  "author": "Matt Hinchliffe",
  "license": "ISC",
  "dependencies": {
    "mri": "^1.1.4",
    "node-fetch": "^2.6.0",
    "semver": "^7.1.0"
  },
  "devDependencies": {
    "jasmine": "^3.5.0",
    "prettier": "^2.0.4"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
